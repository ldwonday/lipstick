.i-sticky-item-header {
  background: #eee;
  font-size: 14px;
  width: 100%;
  height: 32px;
  line-height: 32px
}

.i-sticky-item-content {
  font-size: 14px
}

.i-sticky-title {
  width: 100%;
  box-sizing: border-box;
  background: #eee;
  z-index: 99;
}

.i-sticky-fixed .i-sticky-title {
  position: fixed;
  top: 0
}
